<h2>Last URL</h2>
<table class="table">
  <thead>
    <tr>
      <th>URL</th>
      <th>Short Link</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i < links.length; i++) { %>
      <tr>
      <td><a href="<%= links[i].url %>"><%= links[i].url %></a></td>
      <td><a href="<%= links[i].shortLink %>"><%= links[i].shortLink %></a></td>
      <td><%= links[i].date %></td>
      </tr>
    <% } %>
  </tbody>
</table>
